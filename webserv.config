#asd
http {
  server {
    listen  80;
    server_name localhost;
    location / {
      root YoupiBanane;
      limit_except GET {
        deny all;
      }
      index  youpi.bad_extension;
      # autoindex on;
    }
    location /directory {
      root YoupiBanane;
      limit_except GET {
        deny all;
      }
      index  youpi.bad_extension;
      # autoindex on;
    }
    location /post_body  {
      root YoupiBanane;
      limit_except POST {
        deny all;
      }
    }
    location ~ .bla {
      root YoupiBanane;
      limit_except GET POST DELETE {
        deny all;
      }
      cgi_root ./cgi_tester;
    }
    location /put_test {
      root YoupiBanane;
      limit_except GET {
        deny all;
      }
      index  youpi.bad_extension;
      autoindex on;
    }
  }
}
