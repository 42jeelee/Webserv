#asd
http {
  server {
    listen  80; 
    server_name localhost;
    location / {
        limit_except GET POST DELETE {
          deny all;
        }
        # index  index.html;
        # root html;
        autoindex on;
        error_page 404 404.html;
    }
    location ~ .php {
      root html;
      error_page 404 1index.html;
      cgi_root /opt/homebrew/bin/php-cgi;
    }
  }
    server {
    listen  81; 
    server_name localhost;

    location / {
        index  404.html;
        root html;
        autoindex on;
        error_page 404 1index.html;
        error_page 500 2index.html;
    }
  }
}