#asd
http {
  server {
    listen  80; 
    server_name localhost;
    location / {
        limit_except GET {
          deny all;
        }
        index  youpi.bad_extension;
        root YoupiBanane;
        autoindex on;
    }
    location /directory {
        limit_except GET {
          deny all;
        }
        index  youpi.bad_extension;
        root YoupiBanane;
        autoindex on;
    }
    location /directory/nop {
        limit_except GET {
          deny all;
        }
        index  youpi.bad_extension;
        root YoupiBanane/nop;
        autoindex on;
    }
    location /post_body {
        limit_except POST {
          deny all;
        }
        index  youpi.bad_extension;
        root YoupiBanane/nop;
        autoindex on;
    }
    location ~ .bla {
      root ./;
      cgi_root ./cgi_tester;
    }
  }
}
