#asd
http {
  server {
    listen  80; #1asdasd
    server_name localhost;

    location / {
        # index  index.html;
        root html;
        autoindex on;
        return 300 /index.html;
    }
  }
  server {
    listen  80; #1asdasd
    server_name 2;

    location / {
        index  index.html;
        root html;
        autoindex on;
        
    }
  }
    server {
    listen  81; #1asdasd
    server_name localhost;

    location / {
        index  404.html;
        root html;
        autoindex on;
        error_page 404 1index.html;

        error_page 500 2index.html;
    }
  }
}